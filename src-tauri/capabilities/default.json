{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "http:allow-fetch",
    "core:default",
    "opener:default", 
    "theme:default",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close",
    "upload:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://**"
        },
        {
          "url": "http://**"
        }
      ]
    },
    "fs:default"
  ]
}